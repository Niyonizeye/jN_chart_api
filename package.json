{
  "name": "jnchartapi",
  "version": "1.0.0",
  "description": "for creating chart web",
  "main": "index.js",
  "repository": "git+https://github.com/Niyonizeye/jN_chart_api.git",
  "scripts": {
    "start": "nodemon --exec babel-node index.js",
    "migrate": "npx sequelize-cli db:migrate",
    "undomigrate": "npx sequelize-cli db:migrate:undo",
    "migrations:test": "npx sequelize-cli db:migrate",
    "undomigrate:test": "npx sequelize-cli db:migrate:undo",
    "seeduser": "npx sequelize-cli seed:generate --name demo-user",
    "insertuserdata": "npx sequelize-cli db:seed:all",
    "test": "jest test --coverage",
    "test:watch": "yarn test -- --watch"
  },
  "author": "Jean paul NIYOIZEYE",
